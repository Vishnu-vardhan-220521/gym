package com.movieBuzz;

import java.util.Scanner;
class balayya1 extends Thread{
public void run() {
	for(int i=0;i<10;i++) {
		System.out.println("Balayya "+i);
	}
}
}
class chiru1 extends Thread{
	public void run() {
		for(int i=0;i<10;i++) {
			System.out.println("chiru "+i);
		}
	}
}
public class Movies extends Thread{

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		

	System.out.println("Enter movie name");
	String op=sc.nextLine();
	System.out.println(op);
	if(op=="chiru") {
		chiru1 c=new chiru1();
		c.start();
	}else  {
		balayya1 b=new balayya1();
		b.start();
	}
}
	

}
