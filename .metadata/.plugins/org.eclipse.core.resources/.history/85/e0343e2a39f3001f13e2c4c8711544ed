package com.movieBuzz;

import java.util.Scanner;
class balayya extends Thread{
public void run() {
	for(int i=0;i<10;i++) {
		System.out.println("Balayya "+i);
	}
}
}
class chiru extends Thread{
	public void run() {
		for(int i=0;i<10;i++) {
			System.out.println("chiru "+i);
		}
	}
}
public class Movies extends Thread{

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		

	System.out.println("Enter movie name");
	String op=sc.next();
	System.out.println(op);
	if(op=="chiru") {
		chiru c=new chiru();
		c.start();
	}else  {
		balayya b=new balayya();
		b.start();
	}
}
	

}
